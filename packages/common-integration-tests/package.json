{
  "name": "@gomomento/common-integration-tests",
  "version": "0.0.1",
  "description": "Shared integration tests for Momento JS SDKs",
  "main": "dist/src/index.js",
  "files": [
    "dist/src",
    "dist/package.json"
  ],
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/momentohq/client-sdk-nodejs"
  },
  "scripts": {
    "prebuild": "eslint . --ext .ts",
    "test": "jest",
    "unit-test": "jest unit --passWithNoTests",
    "integration-test": "jest integration --passWithNoTests",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "watch": "tsc -w",
    "build": "tsc",
    "build-with-deps": "cd ../core && npm run build && cd - && tsc",
    "build-without-local-core": "tsc"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.7",
    "@types/node": "16.18.97",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.7.0",
    "jest-extended": "4.0.2",
    "prettier": "2.8.8",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@gomomento/sdk-core": "file:../core",
    "uuid": "9.0.0"
  },
  "engines": {
    "node": ">= 16"
  }
}
