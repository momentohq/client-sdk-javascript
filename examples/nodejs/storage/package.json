{
  "name": "momento-nodejs-example",
  "version": "1.0.0",
  "description": "Getting started with Momento Storage Client",
  "main": "index.ts",
  "scripts": {
    "prebuild": "eslint . --ext .ts",
    "build": "tsc",
    "basic": "tsc && node dist/basic.js",
    "advanced": "tsc && node dist/advanced.js",
    "doc-examples": "tsc && node dist/doc-examples-js-apis.js",
    "validate-examples": "tsc && node dist/doc-example-files/doc-examples-js-apis.js && node dist/doc-example-files/cheat-sheet-main.js && node dist/basic.js && node dist/advanced.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "16.11.4",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@gomomento/sdk": "^1.103.0"
  },
  "engines": {
    "node": ">=10.4.0"
  }
}
